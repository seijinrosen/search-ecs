{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/jsx-runtime",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "prettier"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react", //
    "@typescript-eslint"
  ],
  "rules": {
    "import/order": [
      2,
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "react/jsx-newline": "warn"
  }
}
